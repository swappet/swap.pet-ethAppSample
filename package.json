{
  "name": "swap-pet-eth-app-sample",
  "version": "1.7.2",
  "description": "Swap.Pet app sample of etherum, support truffle, hardhat, coverage,gas report",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-environment": "^0.1.6",
    "@openzeppelin/test-helpers": "^0.5.9",
    "@truffle/hdwallet-provider": "^1.1.1",
    "bip39": "^3.0.2",
    "eth-gas-reporter": "^0.2.19",
    "hardhat": "^2.0.2",
    "inquirer": "^7.3.3",
    "mocha": "^8.2.0",
    "solidity-coverage": "^0.7.11",
    "web3": "^1.3.0"
  },
  "scripts": {
    "ganache": "ganache-cli -e 10000",
    "test": "node scripts/test.js",
    "htest": "npx hardhat test",
    "gas": "npx truffle test",
    "coverage": "npx truffle run coverage",
    "compile": "npx truffle compile",
    "hcompile": "npx hardhat compile",
    "migrate": "node scripts/migrate.js",
    "network": "node scripts/network.js",
    "mocha": "mocha --exit --recursive",
    "build": "npx npm run compile",
    "clean": "npx hardhat clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swappet/swap.pet-ethAppSample.git"
  },
  "keywords": [
    "Swap.Pet",
    "SPTE",
    "SPTC",
    "SPTF",
    "SPTD",
    "pUSD",
    "Oracle"
  ],
  "author": "swap.pet@pm.me",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/swappet/swap.pet-ethAppSample/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/swappet/swap.pet-ethAppSample#readme",
  "jest": {
    "testMatch": [ 
      "<rootDir>/test/*.+(spec|test).+(ts|tsx|js)" 
    ]
  }
}
